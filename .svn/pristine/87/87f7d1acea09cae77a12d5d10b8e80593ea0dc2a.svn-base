}

.wrap_rewards .list_balance .item.withdrawal .status {
    color: #d63131;
    background: #fef3f3
}

.wrap_rewards .list_balance .item.withdrawal .info .point {
    color: #d63131
}

.wrap_rewards .list_balance .item.expired .status {
    color: #5c667b;
    background: #f4f6fa
}

.wrap_rewards .list_balance .item.expired .info .point {
    color: #5c667b
}

/* 모달 공통 */
.ModalBox .title.info {
    margin-bottom: 8px
}

.ModalBox .ModalCont>.txt {
    margin-bottom: 24px;
    color: #8491a7;
    font-size: 14px;
    line-height: 20px
}

.ModalBox .wrap_cont {
    display: grid;
    width: 100%;
    height: 209px;
    place-content: center;
    place-items: center
}

.ModalBox .scroll::-webkit-scrollbar {
    width: 6px;
    height: 8px
}

.ModalBox .scroll::-webkit-scrollbar-track {
    background: #fcfcfc
}

.ModalBox .scroll::-webkit-scrollbar-thumb {
    background: #f2f2f3
}

/* 리워드 안내 모달 */
.ModalBox.layer_reward_tip {
    width: 560px
}

.ModalBox.layer_reward_tip .wrap_img {
    border-radius: 8px;
    background: #f8fafc
}

.ModalBox.layer_reward_tip .wrap_img img {
    display: block
}

/* 쿠폰 상세 모달 */
.ModalBox.layer_product_detail {
    width: 560px;
    height: 448px
}

.ModalBox.layer_product_detail .scroll {
    overflow-y: auto;
    padding-right: 2px;
    width: 100%;
