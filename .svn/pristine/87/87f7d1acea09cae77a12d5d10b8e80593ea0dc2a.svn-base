@charset "UTF-8";
@charset "utf-8"; .wrap_rewards {
    letter-spacing: -1px
}

.wrap_rewards .TabBox {
    padding: 50px 0 20px
}

/* 대시보드 */
.wrap_rewards .reward_dashboard {
    display: grid;
    margin: 56px 0 14px;
    grid-template-columns: 828px 408px;
    gap: 0 12px
}

.wrap_rewards .reward_dashboard .TabBox {
    padding: 50px 0 20px
}

.wrap_rewards .reward_dashboard .status {
    display: grid;
    padding: 24px;
    height: 144px;
    border-radius: 16px;
    box-sizing: border-box;
    background: #4876ef;
    grid-template-areas: "name current" "expired expired";
    place-content: space-between
}

.wrap_rewards .reward_dashboard .status .name {
    display: inline-grid;
    color: #fff;
    font-size: 15px;
    font-weight: bold;
    line-height: 22px;
    grid-template-columns: auto auto;
    grid-area: name;
    gap: 8px
}

.wrap_rewards .reward_dashboard .status .name svg {
    margin-top: 2px;
    width: 20px;
    height: 20px;
    color: #cdedff
}

.wrap_rewards .reward_dashboard .status .current {
    color: #fff;
    font-size: 30px;
    font-weight: bold;
    letter-spacing: 0;
    line-height: 50px;
    grid-area: current
}

.wrap_rewards .reward_dashboard .status .current:after {
    color: #bcd1fc;
    font-size: 22px;
    font-weight: normal;
    content: "M"
}

.wrap_rewards .reward_dashboard .status .expired {
    display: inline-grid;
    padding: 4px 12px;
    border-radius: 8px;
    color: #fff;
    font-size: 14px;
    line-height: 22px;
    text-align: right;
    background: #3769ed;
    grid-template-columns: auto auto;
    grid-area: expired;
    justify-self: flex-end;
    gap: 8px
}

.wrap_rewards .reward_dashboard .status .expired strong {
    font-weight: bold;
    letter-spacing: 0
}

.wrap_rewards .reward_dashboard .status .expired strong:after {
    content: "M"
}

