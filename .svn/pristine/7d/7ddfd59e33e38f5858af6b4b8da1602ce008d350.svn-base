/* 공고 버튼 */
.wrap_list .col_btns .BtnType {
    margin-bottom: 8px;
    padding: 6px 6px 8px;
    width: 100%
}

.wrap_list .col_btns .BtnType._apply_cancel {
    color: var(--gray100);
    border-color: var(--gray80);
    background: var(--white)
}

.wrap_list .col_btns .BtnType._apply_cancel:hover {
    background: var(--gray20)
}

.wrap_list .col_btns .action {
    display: block;
    position: relative;
    width: 140px
}

.wrap_list .col_btns .action .date_end {
    position: absolute;
    top: 0;
    right: 100%;
    padding: 10px 12px 0 0;
    width: 100px;
    color: #4876ef;
    font-size: 14px;
    line-height: 20px;
    letter-spacing: -1px;
    box-sizing: border-box;
    text-align: right
}

.wrap_list .col_btns .action .btn_result {
    color: var(--blue90);
    border-color: var(--blue90);
    background: var(--white)
}

.wrap_list .col_btns .action .btn_result:hover {
    background: var(--blue20)
}

.wrap_list .col_btns .btn_history {
    position: absolute;
    top: 0;
    right: 22px;
    padding: 0 32px 0 16px;
    color: #8491a7;
    font-size: 14px;
    line-height: 40px;
    letter-spacing: -1px
}

.wrap_list .col_btns .btn_history:hover {
    color: #373f57
}

.wrap_list .col_btns .btn_history:after {
    position: absolute;
    top: 50%;
    right: 8px;
    z-index: 1;
    margin-left: -2px;
    width: 5px;
    height: 5px;
    border: 1px solid currentColor;
    border-width: 1px 1px 0 0;
    -webkit-transform: translate(-50%, -50%) rotate(135deg);
    -ms-transform: translate(-50%, -50%) rotate(135deg);
    transform: translate(-50%, -50%) rotate(135deg);
    content: ""
}

.wrap_list .col_btns .btn_reward {
    display: block;
    width: 272px;
    background: #4876ef;
    font-weight: bold;
    line-height: 24px
}

.wrap_list .col_btns .btn_reward:before {
    display: inline-block;
    content: "";
    width: 70px;
    height: 22px;
    margin: 2px 6px 0 0;
    background: url(//www.saraminimage.co.kr/sri/person/ico/img_badge_final.png) no-repeat 0 0;
    vertical-align: top
}

/* 지원 내역 */
.wrap_list .col_history {
    display: none;
    margin-top: 32px;
    padding: 8px 32px;
    width: 100%;
    border-radius: 8px;
    background: #f4f6fa
}

.wrap_list .col_history .timeline li {
    padding: 10px 0;
    border-top: 1px solid #eaedf4
}

.wrap_list .col_history .timeline li:first-child {
    border-top: 0
}

.wrap_list .col_history .timeline .date {
    float: left;
    width: 143px;
    color: #8491a7;
    font-size: 13px;
    line-height: 20px;
    letter-spacing: -.5px
}

.wrap_list .col_history .timeline .desc {
    overflow: hidden;
    display: block;
    color: #8491a7;
    font-size: 14px;
    line-height: 20px;
    letter-spacing: -1px
}

.wrap_list .col_history .timeline .desc>strong {
    display: inline-block;
    padding-right: 20px;
    color: #373f57;
    vertical-align: top
}

.wrap_list .col_history .timeline .desc>span {
    display: inline-block;
    padding-right: 8px;
    vertical-align: top
}

.wrap_list .col_history .timeline .desc .txt {
    display: inline-block;
    position: relative;
    padding: 0 6px;
    color: #373f57;
    line-height: 20px;
    letter-spacing: -1px;
    vertical-align: top
}

.wrap_list .col_history .timeline .desc .txt:before {
    position: absolute;
    top: 9px;
    left: -1px;
    width: 2px;
    height: 2px;
    background: #8491a7;
    content: ""
}

.wrap_list .col_history .timeline .desc .txt:first-child:before {
    content: none
}

.wrap_list .col_history .timeline .desc button.txt {
    text-decoration: underline
}

.wrap_list .list_status .col_summary {
    padding-right: 150px;
    width: 660px
}

.wrap_list .list_status .col_summary .status {
    bottom: 0;
    width: 132px
}

.wrap_list .list_status .col_btns {
    padding: 0 132px 0 108px;
    width: 380px
}

.wrap_list .list_waiting .col_summary {
