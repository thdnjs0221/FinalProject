.layer_offer .ModalCont.change_cont {
    overflow: hidden;
    padding-bottom: 40px;
    height: 63vh;
    min-height: 480px;
    max-height: 600px;
    box-sizing: border-box
}

.layer_offer .ModalCont.change_cont .loading {
    display: flex;
    width: 100%;
    height: 100%;
    background: url(//www.saraminimage.co.kr/sri/person/resume/img_loading_animation.gif) 50% 50% no-repeat;
    align-items: center;
    justify-content: center
}

.layer_offer .ModalCont.change_cont .offer_state_msg {
    display: flex;
    flex-direction: column;
    height: 100%;
    box-sizing: border-box;
    align-items: center;
    justify-content: center
}

.layer_offer .ModalCont.change_cont .offer_state_msg .offer_state_tit, .layer_offer .ModalCont.change_cont .offer_state_msg .offer_state_txt {
    position: relative;
    top: 8px;
    opacity: 0
}

.layer_offer .ModalCont.change_cont .offer_state_msg .offer_state_tit {
    display: block;
    margin-top: 24px;
    color: var(--gray100);
    font-size: 20px;
    letter-spacing: -1px;
    line-height: 28px;
    animation: aniFadeinTxt 0.5s ease-in 0.8s forwards paused
}

.layer_offer .ModalCont.change_cont .offer_state_msg .offer_state_txt {
    margin-top: 8px;
    color: var(--gray80);
    font-size: 13px;
    letter-spacing: -0.5px;
    line-height: 20px;
    animation: aniFadeinTxt 0.5s ease-in 1.2s forwards paused
}

.layer_offer .ModalCont.change_cont .offer_state_msg .ani_draw_circle {
    animation: aniDrawCircle 0.3s ease-in forwards paused;
    stroke-dasharray: 72;
    stroke-dashoffset: 72
}

.layer_offer .ModalCont.change_cont .offer_state_msg .ani_draw_circle2 {
    animation: aniDrawCircle2 0.3s ease-in forwards paused;
    stroke-dasharray: 72;
    stroke-dashoffset: 72
}

.layer_offer .ModalCont.change_cont .offer_state_msg .ani_draw_check {
    animation: aniDrawCheck 0.3s ease-in 0.5s forwards paused;
    stroke-dasharray: 72;
    stroke-dashoffset: 72
}

.layer_offer .ModalCont.change_cont .ani_running .ani_draw_circle, .layer_offer .ModalCont.change_cont .ani_running .ani_draw_circle2, .layer_offer .ModalCont.change_cont .ani_running .ani_draw_check, .layer_offer .ModalCont.change_cont .ani_running .offer_state_tit, .layer_offer .ModalCont.change_cont .ani_running .offer_state_txt {
    -webkit-animation-play-state: running;
    animation-play-state: running
}

@keyframes aniDrawCheck {
    0% {
        stroke-dashoffset: 72;
        /* svg가 나타남*/
    }

    100% {
        stroke-dashoffset: 0
    }
}

@keyframes aniDrawCircle {
    0% {
        opacity: 0;
        fill: none
    }

    100% {
        opacity: 1;
        fill: #deebff
    }
}

@keyframes aniDrawCircle2 {
    0% {
        opacity: 0;
        fill: none
    }

    100% {
        opacity: 1;
        fill: #defaf6
    }
}

@keyframes aniFadeinTxt {
    0% {
        opacity: 0
    }

    100% {
        top: 0;
        opacity: 1
    }
}

