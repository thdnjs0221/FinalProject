@charset "utf-8"; .wrap_my_apply .TabBox {
    padding: 56px 0 20px
}

.wrap_my_apply .status_board {
    display: flex;
    justify-content: space-between;
    margin-bottom: 36px
}

.wrap_my_apply .status_board .item {
    width: 100%;
    box-sizing: border-box
}

.wrap_my_apply .status_board .item+.item {
    margin-left: 12px
}

.wrap_my_apply .status_board .item .dash_tab {
    position: relative;
    padding: 14px 24px;
    width: 100%;
    height: 72px;
    border-radius: 16px;
    color: #8491a7;
    text-align: right;
    letter-spacing: -1px;
    background: #f4f6fa
}

.wrap_my_apply .status_board .item .dash_tab:hover {
    color: #575d6d;
    background: #eff1f5
}

.wrap_my_apply .status_board .item em {
    position: absolute;
    top: 21px;
    left: 24px;
    font-weight: bold;
    font-size: 15px
}

.wrap_my_apply .status_board .item .num {
    display: inline-block;
    position: relative;
    font-weight: bold;
    font-size: 32px;
    line-height: 44px;
    vertical-align: top
}

.wrap_my_apply .status_board .item.new {
    position: relative
}

.wrap_my_apply .status_board .item.new .num {
    color: #5c667b
}

.wrap_my_apply .status_board .item.new .new_txt {
    position: absolute;
    top: 21px;
    right: 48px;
    margin-right: 25px;
    padding: 7px 0 8px;
    width: 153px;
    border-radius: 16px;
    color: #5c667b;
    font-weight: normal;
    font-size: 13px;
    line-height: 18px;
    letter-spacing: -1px;
    text-align: center;
    background: #fff
}

.wrap_my_apply .status_board .item.new .new_txt:hover .TipCont {
    display: block;
    left: 65%;
    z-index: 1
}

.wrap_my_apply .status_board .item>button:hover {
    color: #373f57;
    background: #eaedf4
}

.wrap_my_apply .status_board .item.select>button {
    color: #4876ef;
    background: #eff5ff
}

.wrap_my_apply .status_board .item.select .num {
    color: #4876ef
}

.wrap_list .align {
    position: relative;
    display: flex
}

.wrap_list .align .InpBox+.InpBox, .wrap_list .align .BtnType+.BtnType {
    margin-left: 8px
}

.wrap_list .align .TipBox {
    display: inline-block;
    z-index: 10;
    vertical-align: top
}

.wrap_list .align .TipBox .TipCont.TopLeft {
    left: 4px;
    margin-top: 8px
}

.wrap_list .align .search_area {
    display: inline-block;
    position: relative;
    margin-left: 8px;
    width: 160px
}

.wrap_list .align .search_area input[type=text] {
    padding-right: 36px;
    width: 100%;
    border-color: #d7dce5
}

.wrap_list .align .search_area button {
    display: inline-block;
    position: absolute;
    top: 4px;
    right: 4px;
    width: 32px;
    height: 32px
}

.wrap_list .align .search_area button .icon_search {
    width: 24px;
    height: 24px;
    color: #575d6d
}

.wrap_list .top_btns {
    display: flex;
    justify-content: space-between;
    padding: 12px 0;
    border-bottom: 1px solid #dcdee3
}

.wrap_list .top_btns .period {
    display: inline-block;
    visibility: hidden;
    width: 0;
    height: 0;
    color: #575d6d;
    font-size: 12px;
    line-height: 36px;
    vertical-align: top
}

.wrap_list .top_btns .period.show {
    display: flex;
    visibility: visible;
    width: auto;
    height: auto;
    margin-left: 8px
}

.wrap_list .top_btns .period .TypoBox {
    display: inline-block;
    width: 142px;
    vertical-align: top
}

.wrap_list .top_btns .period .TypoBox .Typo {
    width: 100%
}

.wrap_list .top_btns .period>span {
    margin: 0 4px
}

.wrap_list .top_btns .period .ui-datepicker-trigger {
    display: none
}

.wrap_list .row {
    position: relative;
    padding: 32px 0 32px 208px;
/*     border-bottom: 1px solid #eaedf4; */
    color: #373f57
}

.wrap_list .row:after {
    display: table;
    clear: both;
    content: "";
    table-layout: fixed
}

.wrap_list .row>div {
    position: relative;
    float: left;
    box-sizing: border-box;
    word-break: break-all
}

.wrap_list .row>div a {
    color: currentColor
}

.wrap_list .row .InpBox {
    display: none;
    position: absolute;
    top: 42px;
    left: 0;
    z-index: 1
}

.wrap_list .row .col_date {
    position: absolute;
    top: 0;
    left: 0;
    padding: 32px 0 0 32px;
    color: #8491a7;
    font-size: 13px;
    line-height: 40px;
    letter-spacing: -.5px
}

.wrap_list .bottom_btns {
    display: flex;
    justify-content: space-between;
    margin-top: 12px;
    padding-bottom: 80px
}

.wrap_list .bottom_btns .TipBox+.BtnType {
    margin-left: 8px
}

.wrap_list .bottom_btns .BtnType {
    min-width: 130px;
    color: var(--gray100);
    border-color: var(--gray80);
    background: var(--white)
}

.wrap_list .bottom_btns .BtnType:hover {
    background: var(--gray20)
}

.wrap_list .bottom_btns .BtnType.cancel {
    min-width: 54px
}

.wrap_list .bottom_btns .TipBox .del_apply_row {
    color: #5c667b;
    font-size: 14px;
    line-height: 20px;
    letter-spacing: -1px
}

.wrap_list .bottom_btns .TipBox .del_apply_row dt {
    margin-bottom: 8px;
    font-weight: bold
}

/* 입사지원 내역이 없어요 */
.wrap_list .no_row {
    padding: 72px 0 79px;
    border-bottom: 1px solid #eaedf4;
    text-align: center
}

.wrap_list .no_row .txt {
    padding: 163px 0 14px;
    color: #8491a7;
    font-size: 14px;
    line-height: 24px;
    letter-spacing: -1px;
    background: url(//www.saraminimage.co.kr/sri/person/resume/img_empty_announce.png) 50% 0 no-repeat
}

.wrap_list .no_row .txt b {
    display: block;
    padding-bottom: 4px;
    color: #5c667b;
    font-size: 18px;
    line-height: 32px
}

.wrap_list .no_row .link_go {
    display: inline-block;
    position: relative;
    padding: 10px 14px 10px 0;
    color: #4876ef;
    font-size: 14px;
    letter-spacing: -1px;
    vertical-align: top
}

.wrap_list .no_row .link_go:after {
    position: absolute;
    top: 50%;
    right: 0;
    margin-left: -2px;
    width: 5px;
    height: 5px;
    border: solid currentColor;
    border-width: 1px 1px 0 0;
    -webkit-transform: translate(-50%, -50%) scale(1.1428571429, 1) rotate(45deg);
    -ms-transform: translate(-50%, -50%) scale(1.1428571429, 1) rotate(45deg);
    transform: translate(-50%, -50%) scale(1.1428571429, 1) rotate(45deg);
    content: ""
}

.wrap_list .no_row .link_go:hover {
    color: #3157dd
}

/* 공고 제목 */
.wrap_list .col_summary {
    min-height: 100px
}

.wrap_list .col_summary .corp {
    display: block;
    overflow: hidden;
    padding: 6px 0;
    font-weight: bold;
    font-size: 20px;
    line-height: 28px;
    letter-spacing: -1px;
    white-space: nowrap;
    text-overflow: ellipsis
}

.wrap_list .col_summary .corp>a:hover {
    position: relative
}

.wrap_list .col_summary .corp>a:hover:after {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 1px;
    background: currentColor;
    content: ""
}

.wrap_list .col_summary .flag_reward {
    display: inline-block;
    margin-left: 4px;
    padding: 6px 8px 3px;
    height: 28px;
    box-sizing: border-box;
    color: #363225;
    font-weight: normal;
    font-size: 13px;
    letter-spacing: -.5px;
    line-height: 16px;
    vertical-align: top;
    background: #ffe581
}

.wrap_list .col_summary .flag_reward:after {
    display: inline-block;
    margin-left: 2px;
    width: 16px;
    height: 16px;
    vertical-align: top;
    background: url(//www.saraminimage.co.kr/sri/common/img/img_reward_flag.png) 0 0 no-repeat;
    content: ""
}

.wrap_list .col_summary .recruit {
    padding: 8px 0 12px;
    color: #8491a7;
    font-size: 15px;
    line-height: 24px;
    letter-spacing: -1px
}

.wrap_list .col_summary .recruit>a:hover {
    text-decoration: underline
}

.wrap_list .col_summary .recruit>a:hover .division, .wrap_list .col_summary .recruit>a:hover .TipBox>span {
    text-decoration: underline
}

.wrap_list .col_summary .recruit .removed {
    position: relative;
    padding-left: 44px
}

.wrap_list .col_summary .recruit .removed:before {
    display: inline-block;
    position: absolute;
    top: 1px;
    left: 0;
    width: 36px;
    height: 24px;
    border-radius: 4px;
    background-color: #f4f6fa;
    color: #818b9e;
    font-size: 12px;
    letter-spacing: -1px;
    line-height: 24px;
    text-align: center;
    content: "\C0AD\C81C"
}

.wrap_list .col_summary .recruit .enabled+.TipCont.TopLeft {
    left: 0;
    z-index: 1
}

.wrap_list .col_summary .recruit .TipBox>span {
    display: inline-block;
    height: 24px;
    line-height: 24px
}

.wrap_list .col_summary .recruit .TipBox>span:not(.enabled)+.TipCont {
    display: none !important
}

.wrap_list .col_summary .recruit .TipBox .TipCont .TipTxt {
    color: #5c667b
}

.wrap_list .col_summary .division {
    display: block;
    color: #5c667b
}

.wrap_list .col_summary .txt_status {
    display: block;
    overflow: hidden;
    height: 48px;
    font-weight: bold;
    font-size: 20px;
    line-height: 40px;
    letter-spacing: -1px
}

.wrap_list .col_summary .txt_sub {
    display: block;
    font-size: 15px;
    line-height: 24px;
    letter-spacing: -1px
}

.wrap_list .col_summary .txt_sub.new {
    color: #4876ef;
    font-weight: bold
}

.wrap_list .col_summary .txt_sub.new:after {
    display: inline-block;
    margin-left: 4px;
    width: 5px;
    height: 5px;
    border-radius: 50%;
    vertical-align: top;
    background: #4876ef;
    content: ""
}

