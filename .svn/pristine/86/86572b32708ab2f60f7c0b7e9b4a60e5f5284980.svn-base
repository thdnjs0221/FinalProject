@charset "UTF-8";


/* ---------------------------------------사람인 css 시작 */
/* BI */
#sri_header .bi {
    display: block;
    position: relative
}

#sri_header .bi .frame {
    display: block;
    overflow: hidden;
    padding-left: 14px;
    width: 140px;
    height: 70px
}

#sri_header .bi .frame svg {
    display: block;
    width: 126px;
    height: 40px;
    color: #b4c0d3
}

/* family service logo */
.logo_family_service {
    display: block
}

