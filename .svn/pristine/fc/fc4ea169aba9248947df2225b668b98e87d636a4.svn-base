/* member_invite */
/* common */
.wrap_member_invite {
}

.wrap_member_invite .item_invite:after {
    display: table;
    clear: both;
    content: "";
    table-layout: fixed
}

.wrap_member_invite .item_invite .item {
    float: left;
    margin-right: 20px
}

.wrap_member_invite .item_invite .item label {
    display: block;
    margin-bottom: 5px;
    color: #222;
    font-size: 14px;
    letter-spacing: -1px;
    line-height: 18px
}

.wrap_member_invite .item_invite .item label .required {
    margin-left: 6px;
    color: #f00;
    font-size: 12px;
    line-height: 18px
}

.wrap_member_invite .txt_ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
}

.wrap_member_invite .area_btn {
    margin-top: 24px;
    text-align: right
}

.wrap_member_invite .area_noti {
    margin-top: 80px;
    padding: 24px 30px;
    border-radius: 6px;
    color: #666;
    font-size: 14px;
    line-height: 26px;
    letter-spacing: -1px;
    background-color: #fafbfc
}

/* invite_form */
.wrap_invite_form .area_list_invite {
    border-bottom: 1px solid #eee
}

.wrap_invite_form .area_list_invite .item_invite {
    margin-bottom: 32px;
    padding-top: 24px;
    border-top: 1px solid #eee
}

.wrap_invite_form .area_list_invite .item_invite .item input {
    width: 180px
}

.wrap_invite_form .area_list_invite .item_invite .item input.inp_mail {
    width: 440px
}

.wrap_invite_form .area_list_invite .item_invite .btn_del {
    margin-top: 24px;
    width: 100px
}

.wrap_invite_form .area_btn_send {
    text-align: center
}

.wrap_invite_form .area_btn_send .btn_send {
    margin-top: 20px;
    width: 240px
}

/* manage */
.wrap_manage .box_invite {
    position: relative;
    padding: 24px 40px;
    height: 205px;
    box-sizing: border-box;
    border-radius: 6px;
    background-color: #f2f6f9
}

.wrap_manage .box_invite h2 {
    color: #000;
    font-size: 16px;
    font-weight: bold;
    line-height: 22px;
    letter-spacing: -1px
}

.wrap_manage .box_invite .item_invite {
    margin-top: 16px
}

.wrap_manage .box_invite .item_invite .item {
    margin-right: 10px
}

.wrap_manage .box_invite .item_invite .item input {
    width: 190px
}

.wrap_manage .box_invite .item_invite .item input.inp_mail {
    width: 360px
}

.wrap_manage .box_invite .btn_invite {
    margin-top: 24px;
    width: 110px
}

.wrap_manage .box_invite .txt_link {
    position: absolute;
    right: 40px;
    bottom: 24px;
    color: #666;
    font-size: 13px;
    line-height: 18px;
    text-decoration: underline;
    letter-spacing: -1px
}

.wrap_manage .box_list_manage {
    margin-top: 30px
}

.wrap_manage .box_list_manage .total_num {
    color: #444;
    font-size: 14px;
    letter-spacing: -1px
}

.wrap_manage .box_list_manage .tbl_invite_manage a {
    display: inline-block;
    width: 100%
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_name {
    display: inline-block;
    max-width: 118px;
    line-height: 20px;
    box-sizing: border-box
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_id {
    display: block;
    margin-top: 4px;
    color: #666;
    font-size: 13px
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_weit {
    display: block;
    color: #2365f2;
    font-size: 13px
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_day {
    display: block;
    color: #666;
    font-size: 13px
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_type {
    display: inline-block;
    padding: 2px 6px;
    border: 1px solid #ddd;
    box-sizing: border-box;
    border-radius: 4px;
    color: #666;
    font-size: 11px;
    line-height: 15px;
    letter-spacing: -1px;
    vertical-align: top;
    background-color: #fff
}

.wrap_manage .box_list_manage .tbl_invite_manage .box_empty {
    padding: 115px 0;
    color: #666;
    font-size: 14px;
    line-height: 24px
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_master {
    display: inline-block
}

.wrap_manage .box_list_manage .tbl_invite_manage .user_master .user_name {
    max-width: calc(120px - 48px)
}

/* activity_log */
.wrap_activity_log .box_filter {
    margin-bottom: 10px;
    text-align: right
}

.wrap_activity_log .box_filter .inpSel {
    width: 146px
}

.wrap_activity_log .box_filter .inpTypo {
    width: 188px
}

.wrap_activity_log .box_filter .area_calendar {
    display: inline-block
}

.wrap_activity_log .box_filter .area_calendar .inpTypo {
    width: 135px
}

/* modal */
.modal_user_info {
    margin: -325px 0 0 -265px;
    width: 530px;
    max-height: 650px
}

.modal_user_info .modalBtn .btn_invite {
    width: 150px
}
