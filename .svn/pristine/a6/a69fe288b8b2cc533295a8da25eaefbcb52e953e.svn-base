/* 프로그레스바 */
#progress_bar {
    position: absolute;
    top: 52px;
    left: 0;
    z-index: 10;
    width: 720px;
    height: 4px;
    border-radius: 4px;
    background-color: #eff5ff
}

#progress_bar:before {
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    height: 4px;
    background-color: #eff5ff;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    content: ""
}

#progress_bar:after {
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    height: 4px;
    border-radius: 4px;
    background-color: #bcd1fc;
    transition: -webkit-transform 0.5s ease;
    transition: transform 0.5s ease;
    transition: transform 0.5s ease, -webkit-transform 0.5s ease;
    -webkit-transform: scaleX(0) translateY(-50%);
    transform: scaleX(0) translateY(-50%);
    -webkit-transform-origin: left center;
    transform-origin: left center;
    content: ""
}

#progress_bar.step1:after {
    -webkit-transform: scaleX(0.30) translateY(-50%);
    transform: scaleX(0.30) translateY(-50%)
}

#progress_bar.step2:after {
    -webkit-transform: scaleX(0.60) translateY(-50%);
    transform: scaleX(0.60) translateY(-50%)
}

#progress_bar.step3:after {
    -webkit-transform: scaleX(0.90) translateY(-50%);
    transform: scaleX(0.90) translateY(-50%)
}

#progress_bar.step4:after {
    -webkit-transform: scaleX(1) translateY(-50%);
    transform: scaleX(1) translateY(-50%)
}

#progress_bar.step1.step1_sub {
    top: 52px
}

#progress_bar.step1.step1_sub:after {
    -webkit-transform: scaleX(0.30) translateY(-50%);
    transform: scaleX(0.30) translateY(-50%)
}

#progress_bar.step1.step1_sub .step_bar.active .progress_number {
    left: 30%
}

#progress_bar .progress_number {
    display: none;
    position: absolute;
    top: 12px;
    margin-left: -29px;
    padding: 4px 6px;
    width: 58px;
    border-radius: 4px;
    color: #fff;
    font-size: 10px;
    letter-spacing: -0.5px;
    line-height: 12px;
    text-align: center;
    background-color: #6d9cfa;
    box-shadow: 0 0 8px rgba(0, 0, 0, 0.1)
}

#progress_bar .progress_number:before {
    display: block;
    position: absolute;
    top: -4px;
    left: 24px;
    width: 8px;
    height: 8px;
    background-color: #6d9cfa;
    transform: rotate(-45deg);
    content: ""
}

#progress_bar .step_bar.active .progress_number {
    display: inline-block
}

#progress_bar .step_bar.active.step1 .progress_number {
    left: 30%
}

#progress_bar .step_bar.active.step2 .progress_number {
    left: 60%
}

#progress_bar .step_bar.active.step3 .progress_number {
    left: 90%
}

