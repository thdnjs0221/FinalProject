.area_feedback, .recruilt_list_wrap .common_recruit_list td.area_feedback {
    padding: 50px 0
}

.box_feedback {
    display: none;
    position: relative;
    z-index: 1;
    width: 100%;
    border: 4px solid #e8eeff;
    box-sizing: border-box;
    text-align: center
}

.box_feedback.show {
    display: block
}

.box_feedback .inner {
    overflow: hidden;
    padding: 12px 50px
}

.box_feedback .txt_feedback {
    display: inline-block;
    padding: 4px 0 5px 0;
    color: #485dd2;
    font-size: 16px;
    font-weight: normal;
    letter-spacing: -1px;
    vertical-align: top
}

.box_feedback .txt_feedback span {
    font-weight: bold
}

.box_feedback .wrap_btn {
    display: inline-block;
    overflow: hidden;
    padding-left: 17px;
    vertical-align: top
}

.box_feedback .btn_choice {
    margin-left: 4px;
    width: 115px;
    height: 32px;
    border: 1px solid #6e82f3;
    box-sizing: border-box;
    color: #6e82f3;
    font-size: 14px;
    letter-spacing: -1px;
    line-height: 29px;
    text-align: center
}

.box_feedback .btn_choice:first-child {
    margin-left: 8px
}

.box_feedback .txt_thankyou .txt_feedback:after {
    display: inline-block;
    margin: 0 0 0 11px;
    width: 20px;
    height: 20px;
    vertical-align: top;
    background: url(//www.saraminimage.co.kr/ui/search/sp/ico_search_feed.png) no-repeat;
    content: ""
}

.box_feedback.type2 .wrap_btn {
    display: block;
    padding: 10px 0 0 0;
    text-align: center
}

.box_feedback.type2 .box_goodtext {
    position: relative
}

.box_feedback.level2 {
    padding: 0
}

.box_feedback .tit_opinion {
    position: relative;
    padding-left: 43px;
    width: 100%;
    height: 41px;
    box-sizing: border-box;
    line-height: 22px;
    text-align: left;
    background-color: #e8eeff;
}

.box_feedback .tit_opinion:before {
    display: inline-block;
    position: absolute;
    top: 3px;
    left: 7px;
    width: 30px;
    height: 30px;
    vertical-align: top;
    background: url(//www.saraminimage.co.kr/sri/event/wmg-15802/ico_appr.png) no-repeat 8px 8px;
    content: ""
}

.box_feedback .input_opinion {
    margin: 26px 26px 14px 26px;
    padding: 5px 8px;
    border: 1px solid #e2e2eb;
    box-sizing: border-box;
    background-color: #f8f8f8
}

.box_feedback .input_opinion.focus {
    border-color: #999;
    background-color: #fff
}

.box_feedback .sri_textarea {
    display: block;
    overflow-x: hidden;
    overflow-y: auto;
    width: 100%;
    height: 93px;
    border: 0 none;
    outline: none;
    color: #444;
    font-size: 13px;
    letter-spacing: -1px;
    line-height: 1.5;
    background-color: transparent;
    resize: none;
}

.box_feedback .sri_textarea::-webkit-input-placeholder {
    color: #a3a3a3
}

.box_feedback .sri_textarea:-ms-input-placeholder {
    color: #a3a3a3
}

.box_feedback .sri_textarea::placeholder {
    color: #a3a3a3
}

.box_feedback fieldset {
    padding-bottom: 25px
}

.box_feedback .btn_submit {
    float: right;
    margin-right: 26px;
    width: 110px;
    height: 45px;
    color: #fff;
    font-family: "Malgun Gothic",dotum,gulim,sans-serif;
    font-size: 14px;
    letter-spacing: -1px;
    background-color: #6e82f3;
}

.box_feedback .btn_close {
    overflow: hidden;
    position: absolute;
    top: 9px;
    right: 7px;
    padding: 10px
}

.box_feedback .btn_close:after {
    display: block;
    width: 18px;
    height: 18px;
    background: url(//www.saraminimage.co.kr/ui/search/sp/ico_search_feed.png) no-repeat -30px -30px;
    content: ""
}

.box_feedback.level2 .btn_close {
    top: 0
}

.page_nation+.area_feedback, .recruit_content_bottom+.area_feedback, .company_list.graph_grid .area_feedback {
    padding-bottom: 0
}

.public_recruit_calendar_wrap+.area_feedback {
    padding-top: 0
}

