    margin-top: 16px;
    padding: 0 20px;
    width: 294px;
    height: 64px;
    border: 1px solid #eff5ff;
    border-radius: 8px;
    box-sizing: border-box;
    color: #475067;
    font-size: 14px;
    letter-spacing: -1px;
    line-height: 20px;
    background: #eff5ff;
    align-items: center;
}

