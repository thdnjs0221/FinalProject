.resume_view .user_lock {
    display: flex;
    padding: 0 28px;
    width: 810px;
    height: 56px;
    border: 1px solid var(--gray30);
    border-radius: 8px;
    box-sizing: border-box;
    color: var(--gray100);
    font-size: 14px;
    letter-spacing: -1px;
    line-height: 20px;
    align-items: center;
}

.resume_view .user_lock:before {
    display: inline-block;
    margin-right: 8px;
    width: 16px;
