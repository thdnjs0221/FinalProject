/* 포트폴리오 */
.port_list {
    display: flex;
    margin-bottom: 16px;
    flex-wrap: wrap;
    gap: 16px 10px;
}

.port_list .box {
    overflow: hidden;
    position: relative;
    width: 255px;
    height: 255px;
    border: 1px solid var(--gray40);
    border-radius: 8px;
    box-sizing: border-box
}

