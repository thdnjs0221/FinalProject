    margin: 8px 0 0 0
}

.resume_view .user_info li:before {
    display: inline-block;
    margin-right: 12px;
    width: 16px;
    height: 16px;
    content: "";
    flex-shrink: 0;
}

.resume_view .user_info li span {
    overflow: hidden;
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap
}

