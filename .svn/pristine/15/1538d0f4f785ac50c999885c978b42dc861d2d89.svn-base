.resume_con .certification_link {
    margin-left: auto;
    display: flex;
    align-items: center;
    height: 22px;
    color: var(--blue90);
    font-size: 15px;
    line-height: 22px;
    letter-spacing: -1px;
    text-decoration: underline
}

